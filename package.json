{
  "name": "whitelabelwallet",
  "productName": "White Label Wallet",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "codeparticle",
    "email": "web-accounts@codeparticle.com"
  },
  "description": "White Label Wallet is an open-source exchange wallet which can be extended for any crypto-currency",
  "homepage": "https://bitbucket.org/CodeParticle/whitelabelwallet",
  "contributors": [
    {
      "name": "codeparticle",
      "email": "web-accounts@codeparticle.com"
    }
  ],
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "main": "dist/main.js",
  "build": {
    "appId": "whitelabelwallet",
    "files": [
      "dist/*",
      "node_modules/",
      "templates/",
      "backgrounds/",
      "icons/",
      "package.json"
    ],
    "directories": {
      "buildResources": "./",
      "output": "installers"
    },
    "fileAssociations": {
      "ext": "png",
      "icon": "./icons/icon.png"
    },
    "mac": {
      "category": "public.app-category.finance",
      "type": "distribution"
    },
    "dmg": {
      "title": "${productName} ${version}",
      "background": "backgrounds/installer-bg.tiff",
      "contents": [
        {
          "x": 176,
          "y": 251
        },
        {
          "x": 360,
          "y": 251,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office"
    },
    "appImage": {
      "artifactName": "${productName}-${version}.${ext}"
    },
    "deb": {
      "artifactName": "${productName}-${version}.${ext}"
    },
    "nsis": {
      "artifactName": "${productName}-${version}.${ext}"
    },
    "win": {
      "icon": "icons/icon.ico",
      "target": "nsis",
      "rfc3161TimeStampServer": "http://timestamp.comodoca.com/rfc3161",
      "timeStampServer": "http://timestamp.verisign.com/scripts/timstamp.dll"
    }
  },
  "keywords": [
    "whitelabelwallet",
    "wallet",
    "windows",
    "mac",
    "osx",
    "linux",
    "desktop",
    "electron"
  ],
  "scripts": {
    "start": "yarn run dev",
    "dev": "run-p web node-server:dev mock-server:dev",
    "dev:electron": "run-p electron node-server:dev mock-server:dev",
    "dev:mobile": "yarn build && npx cap serve",
    "electron": "TYPE=electron NODE_ENV=development electron main.js & TYPE=electron NODE_ENV=development webpack-dev-server --config config/webpack.electron-dev.config.js --progress --colors",
    "electron-mock": "COIN=mock MODE=mock yarn run electron",
    "web": "NODE_ENV=development webpack-dev-server --config config/webpack.dev.config.js --progress --colors",
    "web-mock": "COIN=mock MODE=mock yarn run web",
    "build": "NODE_ENV=production webpack --display-error-details --config config/webpack.prod.config.js --progress --colors",
    "build:mobile": "yarn build && npx cap copy",
    "build:mobile-mock": "COIN=mock MODE=mock yarn run build:mobile",
    "build:electron": "concurrently 'npm run build:electron-main' 'npm run build:electron-renderer'",
    "build:electron-main": "cross-env TYPE=electron NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config ./config/webpack.config.main.prod.js --colors",
    "build:electron-renderer": "cross-env TYPE=electron NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config ./config/webpack.config.renderer.prod.js --colors",
    "pack:electron": "node ./ci/misc/electron-pack.js",
    "build-pack:electron": "npm run build:electron && npm run pack:electron",
    "open:android": "npx cap update android && npx cap sync android && npx cap open android",
    "open:ios": "npx cap update ios && npx cap sync ios && npx cap open ios",
    "mock-server": "node src/server/json-server.js",
    "mock-server:dev": "nodemon -L --watch src/server/json-server.js --watch src/server/db.json src/server/json-server.js",
    "node-server": "node src/server",
    "node-server:dev": "nodemon -L --watch src/server/index.js src/server/index.js",
    "manage:translations": "react-intl-cra 'src/translations/keys/**/*.{js,jsx}' -o 'src/translations/messages/messages.json' && node ./ci/misc/translation-runner.js",
    "postinstall": "electron-builder install-app-deps package.json",
    "test": "jest --env=jsdom",
    "test:unit": "jest --env=jsdom tests/unit",
    "test:e2e": "jest --env=jsdom tests/e2e/",
    "test:e2e-nogui": "NODE_E2E_NO_GUI=true npm run test:e2e",
    "test:update": "NODE_ENV=test jest -u",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src ./plugins ./tests **/*.jsx ./ci && npm run lint-css",
    "lint-css": "stylelint '**/*.scss'",
    "lint-css-autofix": "stylelint '**/*.scss' --fix",
    "lint-autofix": "run-p lint-css-autofix eslint-autofix",
    "eslint-autofix": "eslint ./src ./plugins ./tests ./ci --fix",
    "pre-push": "npm run lint && npm run test:unit && npm run test:e2e",
    "mock-pipeline": "bash ./ci/misc/mock-pipeline.sh",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch"
  },
  "standard-version": {
    "scripts": {
      "posttag": "git push --follow-tags origin master"
    },
    "types": [
      {
        "type": "feature",
        "section": "Features"
      },
      {
        "type": "bugfix",
        "section": "Bug Fixes"
      },
      {
        "type": "test",
        "hidden": "Tests"
      },
      {
        "type": "misc",
        "hidden": true
      }
    ],
    "commitUrlFormat": "{{host}}/{{owner}}/{{repository}}/commits/{{hash}}"
  },
  "jest": {
    "testEnvironment": "node",
    "testURL": "http://localhost:8080/",
    "setupTestFrameworkScriptFile": "./src/config/setup-tests.js",
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest",
      "^api": "./src/api",
      "^app": "./src/app",
      "^components": "./src/components",
      "^config": "./src/config",
      "^containers": "./src/e2e",
      "^electron": "./src/electron",
      "^lib": "./src/lib",
      "^pages": "./src/pages",
      "^rdx": "./src/rdx",
      "^server": "./src/server",
      "^translations": "./src/translations",
      "^window": "./src/window",
      "^e2e": "./tests/e2e",
      "^package.json": "./package.json"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
